    "ModerateDemented = np.array(glob(\"./data/Alzheimer_s_Dataset/train/ModerateDemented/*\"))\n",
    "NonDemented = np.array(glob(\"./data/Alzheimer_s_Dataset/train/NonDemented/*\"))\n",
    "VeryMildDemented = np.array(glob(\"./data/Alzheimer_s_Dataset/train/VeryMildDemented/*\"))\n",
    "\n",
    "\n",
    "MildDemented_test = np.array(glob(\"./data/Alzheimer_s_Dataset/test/MildDemented/*\"))\n",
    "ModerateDemented_test = np.array(glob(\"./data/Alzheimer_s_Dataset/test/ModerateDemented/*\"))\n",
    "NonDemented_test = np.array(glob(\"./data/Alzheimer_s_Dataset/test/NonDemented/*\"))\n",
    "VeryMildDemented_teat = np.array(glob(\"./data/Alzheimer_s_Dataset/test/VeryMildDemented/*\"))\n",
